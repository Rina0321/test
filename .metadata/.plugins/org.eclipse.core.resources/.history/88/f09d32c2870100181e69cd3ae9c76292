
public class Person {
  private String name=null;
  private int age=0;


  Person (String name,int age){
	  this.name=name;
	  this.age=age;

  }

  public String getName(){
	  return this.name;}

  public void setName(String name){
	  this.name=name;}

  public void getName(){
      this.age=age;}

  public void setName(int age){
      this.age=age;}
}